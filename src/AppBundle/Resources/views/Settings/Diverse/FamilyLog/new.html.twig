{% extends "base.html.twig" %}

{% form_theme form 'AppBundle:Form:bootstrap_3_horizontal_layout.html.twig' %}

{% block title %}
    {{ '%entity% creation'|trans({'%entity%': 'gestock.settings.diverse.familylog'|trans}, 'admin') }} - {{ parent() }}
{% endblock %}

{% block body_title %}
    <span class="glyphicon glyphicon-tag"></span>
    {{ '%entity% creation'|trans({'%entity%': 'gestock.settings.diverse.familylog'|trans}, 'admin') }}
{% endblock %}

{% block glstock_body %}
    {% include 'AppBundle:Settings/Diverse/FamilyLog:form.html.twig' %}
        
    <div class="btn-group record_actions">
        {% include 'homeButton.html.twig' %}
        <a class="btn btn-info btn-list" href="{{ path('familylog') }}">
            {{ 'gestock.return_list'|trans }}
        </a>

    </div>

{% endblock %}
