{# src\Glsr\GestockBundle\Resources\views\Gestock\Article\index.html.twig #}

{% extends "GlsrGestockBundle::layout.html.twig" %}

{% block title %}
    Settings - {{ parent() }}
{% endblock %}

{% block body_title %}
    {{ 'glsr.gestock.settings.form.title'|trans }}
{% endblock %}

{% block glstock_body %}

    {% include 'GlsrGestockBundle:Gestock/Settings:form.html.twig' %}

    <p>{{ 'glsr.gestock.settings.form.caution'|trans }}</p>

    <p>
        <a href="#" onclick="window.history.back();return false;" class="btn btn-default" role="button">
            <i class="glyphicon glyphicon-chevron-left"></i>
            {{ 'glsr.gestock.settings.return'|trans }}
        </a>
    </p>
{% endblock %}
