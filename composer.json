{
    "name": "dev_int/glsr",
    "type": "project",
    "description": "The GLSR Application, managing a restaurant stocks",
    "homepage": "https://www.glsr.fr",
    "license": "MIT",
    "autoload": {
        "psr-4": { "": "src" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "authors":  [
        {
            "name": "Laurent QuÃ©tier",
            "email": "lq@dev-int.net"
        },
        {
            "name": "Glsr Community",
            "homepage": "https://github.com/Dev-Int/glsr/graphs/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/symfony": "3.4.*",
        "doctrine/orm": "~2.4,>=2.4.5",
        "doctrine/dbal": "~2.5",
        "doctrine/doctrine-bundle": "~1.5",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/polyfill-iconv": "~1.0",
        "symfony/polyfill-php56": "~1.0",
        "symfony/polyfill-php71": "~1.0",
        "symfony/polyfill-php72": "~1.0",
        "symfony/translation": "3.4.*",
        "symfony/config": "3.4.*",
        "symfony/monolog-bundle": "^3.1.0",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "~3.0",
        "symfony/assetic-bundle": "~2.8",
        "incenteev/composer-parameter-handler": "~2.0",
        "doctrine/doctrine-fixtures-bundle": "2.3.*",
        "doctrine/data-fixtures": "1.1.*",
        "twig/extensions": "1.3.*",
        "symfony/intl": "3.4.*",
        "jms/security-extra-bundle": "1.6.*",
        "stof/doctrine-extensions-bundle": "1.2.*",
        "gedmo/doctrine-extensions": "^2.3.1",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "knplabs/knp-markdown-bundle": "~1.3",
        "knplabs/knp-snappy-bundle": "1.5.*",
        "knplabs/knp-paginator-bundle":"2.5.*",
        "knplabs/knp-menu-bundle": "^2.0",
        "lexik/form-filter-bundle": "~5.0",
        "misd/phone-number-bundle": "~1.0",
        "beberlei/DoctrineExtensions": "1.0.11"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*",
        "symfony/phpunit-bridge": "3.4.*"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.1"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.4-dev"
        }
    }
}
