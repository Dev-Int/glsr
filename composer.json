{
    "name": "dev_int/glsr",
    "type": "project",
    "description": "The GLSR Application, managing a restaurant stocks",
    "homepage": "https://www.glsr.fr",
    "license": "proprietary",
    "authors":  [
        {
            "name": "Laurent Qu√©tier",
            "email": "info@developpement-interessant.com"
        },
        {
            "name": "Glsr Community",
            "homepage": "https://github.com/Dev-Int/glsr/graphs/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/DoctrineExtensions": "^1.2",
        "doctrine/dbal": "^2.9",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/orm": "^2.6",
        "friendsofsymfony/user-bundle": "^2.1",
        "gedmo/doctrine-extensions": "^2.4",
        "knplabs/knp-menu-bundle": "^2.2",
        "knplabs/knp-paginator-bundle": "^2.8",
        "knplabs/knp-snappy-bundle": "^1.6",
        "misd/phone-number-bundle": "^1.3",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^5.2",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "3.4.*",
        "symfony/dotenv": "3.4.*",
        "symfony/flex": "^1.1",
        "symfony/form": "3.4.*",
        "symfony/framework-bundle": "3.4.*",
        "symfony/routing": "3.4.*",
        "symfony/security-bundle": "3.4.*",
        "symfony/security-csrf": "3.4.*",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/templating": "3.4.*",
        "symfony/translation": "3.4.*",
        "symfony/twig-bundle": "3.4.*",
        "symfony/webpack-encore-bundle": "^1.1",
        "symfony/yaml": "3.4.*",
        "twig/extensions": "^1.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "3.4.*"
        }
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "phpunit/phpunit": "^8.0"
    }
}
